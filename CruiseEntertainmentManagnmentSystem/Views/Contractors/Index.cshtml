@*@model IEnumerable<CruiseEntertainmentManagnmentSystem.Models.Contractor>*@
<script>
    $(document).ready(function () {
        $('#btnWardrobe').click(function(){
            $('#btnWardrobe').toggleClass("dynamic");
            $('#btnDirectors').removeClass("dynamic");
            $('#btnProduction').removeClass("dynamic");
        });
        $('#btnProduction').click(function () {
            $('#btnWardrobe').removeClass("dynamic");
            $('#btnProduction').addClass("dynamic");
            $('#btnDirectors').removeClass("dynamic");
        });
        $('#btnDirectors').click(function () {
            $('#btnWardrobe').removeClass("dynamic");
            $('#btnProduction').removeClass("dynamic");
            $('#btnDirectors').addClass("dynamic");
        });
    });
</script>

<style>

    .tabbtn{
    width: 86px;
    margin-bottom: 0px;
    margin-right: 0px;
    border-radius: 0;
    border: 0px;
    /*border-right: 5px solid #FFFFFF;*/
    background: #828282;
    color: white;
    border-top-left-radius:3px;
    border-top-right-radius:3px;
    }
    .dynamic{
    background:aliceblue;
    color:#5692D1;
    border:1px solid #BBB4B4;
    border-top-left-radius:3px;
    border-top-right-radius:3px;
    border-bottom:0px;
    margin-right: 0px;
    }
</style>


@{
    ViewBag.Title = "Contractors";
}

<div class="row">

    <h3 class="text-center page-heading">@ViewBag.Title</h3>

    <div style="border-bottom: 1px solid #DEDDDD;height:38px;vertical-align:top;">
        <input type="button" value="Wardrobe" id="btnWardrobe" class="tabbtn" />
        <input type="button" value="Production" id="btnProduction" class="tabbtn" />
        <input type="button" value="Directors" id="btnDirectors" class="tabbtn" />
    </div>
    <br />
    <a href="#" onclick="goToCreate('/Contractors/Create?option=@ViewBag.Option')" class="create-link" id="createnew">Create New</a>
       @*@Html.ActionLink("Create New", "Create", null, new { @class = "create-link" })*@
   
    <div id="dataContainer">
        @Html.Action("Contractors",new{option=@ViewBag.Option})
    </div>


</div>



<script>

   

</script>







